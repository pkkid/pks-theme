{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    // https://code.visualstudio.com/api/references/theme-color
    "activityBar.background": "#1E282D",
    "activityBar.border": "#0004",
    "activityBarBadge.background": "#455",
    "breadcrumb.background": "#111",
    "breadcrumb.foreground": "#678a",
    "dropdown.background": "#0002",
    "dropdown.listBackground": "#182125",
    "editor.background": "#111",
    "editor.foreground": "#aaa",
    "editor.inactiveSelectionBackground": "#444",
    "editor.lineHighlightBackground": "#111",
    "editor.selectionBackground": "#5579",
    "editor.selectionHighlightBackground": "#3336",
    "editorBracketMatch.border": "#fff2",
    "editorGroupHeader.tabsBackground": "#182125",
    "editorIndentGuide.activeBackground": "#222",
    "editorIndentGuide.background": "#111",
    "editorLineNumber.activeForeground": "#666",
    "editorLineNumber.foreground": "#444",
    "editorRuler.foreground": "#211",
    "focusBorder": "#0000",
    "input.background": "#0002",
    "list.activeSelectionBackground": "#263238",
    "list.activeSelectionForeground": "#75BAB4",
    "list.focusBackground": "#263238",
    "list.focusForeground": "#75BAB4",
    "list.highlightForeground": "#ccc",
    "list.inactiveSelectionBackground": "#263238",
    "list.inactiveSelectionForeground": "#75BAB4",
    "sideBar.background": "#182125",
    "sideBar.border": "#000",
    "sideBar.foreground": "#678",
    "sideBarSectionHeader.background": "#111",
    "sideBarSectionHeader.border": "#111",
    "sideBarSectionHeader.foreground": "#aaa",
    "sideBarTitle.foreground": "#eee",
    "statusBar.background": "#1E282D",
    "statusBar.debuggingBackground": "#055",
    "statusBar.debuggingForeground": "#ddd",
    "statusBar.foreground": "#89a",
    "statusBar.noFolderBackground": "#1E282D",
    "tab.activeBackground": "#0000",
    "tab.activeBorderTop": "#8dc",
    "tab.activeForeground": "#ddd",
    "tab.inactiveBackground": "#0000",
    "tab.inactiveForeground": "#cff4",
    "terminal.background" : "#111",
    "terminal.foreground" : "#0f0",
  },
  "tokenColors": [{
      "name": "Red",
      "settings": {"foreground": "#d33"},
      "scope": [
        "beginning.punctuation.definition.list.markdown",
        "constant.other.caps.python",
        "entity.name.function.js",
        "entity.name.function",
        "entity.name.tag",
        "entity.name.variable",
        "meta.object-literal.key.js",
        "meta.tag.any.html",
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "source.css meta.brace",
        "source.python",
        "source",
        "string.unquoted.tag-string",
        "support.function",
        "support.other.django.module",
        "support.type.property-name",
        "support.type.property-name",
        "support.type.vendored.property-name",
        "support.type.vendored.property-name",
        "support.variable",
        "variable.css",
        "variable.language",
        "variable.other.less",
        "variable.scss",
        "variable",
      ],
    },{
      "name": "Pink",
      "settings": {"foreground": "#d77"},
      "scope": [
        "entity.tag.tagbraces",
        "keyword.operator.assignment",
        "keyword",
        "source.css entity.other.attribute-name.pseudo-class",
        "source.css keyword.operator",
      ],
    },{
      "name": "Pink (lighter)",
      "settings": {"foreground": "#e55"},
      "scope": [
        "python punctuation.definition.list",
        "source.python punctuation.definition.arguments",
        "source.python punctuation.definition.dictionary",
        "entity.other.attribute-name.html",
      ],
    },{
      "name": "Peach",
      "settings": {"foreground": "#fc8"},
      "scope": [
        "entity.name.function",
        "entity",
        "source.css entity.name.tag",
        "source.css entity.other.attribute-name.class",
      ],
    },{
      "name": "Orange",
      "settings": {"foreground": "#f90"},
      "scope": [
        "constant.language.python",
        "entity.other.inherited-class",
        "source.css support.constant.property-value.css",
        "constant.language.json.comments",
      ],
    },{
      "name": "Green",
      "settings": {"foreground": "#292"},
      "scope": [
        "comment",
        "keyword.codetag.notation",
        "string.quoted.docstring.multi.python",
      ],
    },{
      "name": "Blue",
      "settings": {"foreground": "#08c"},
      "scope": [
        "keyword.control.tag-name",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator",
        "source.css entity.other.attribute-name.id",
        "source.css keyword.control.html",
        "storage.type.function",
        "storage",
        "support",
      ],
    },{
      "name": "Grey",
      "settings": {"foreground": "#ccc"},
      "scope": [
        "constant.character.entity",
        "constant.numeric",
        "constant",
        "source.css constant.numeric",
        "source.css constant",
        "source.css keyword",
        "string.regexp",
        "text.html.django storage.type.attr",
        "text.html.django storage.type.templatetag",
        "text.html",
        "text.xml",
      ],
    },{
      "name": "Grey (darker)",
      "settings": {"foreground": "#aaa"},
      "scope": [
        "string.quoted.double",
        "string",
      ],
    }]
}
